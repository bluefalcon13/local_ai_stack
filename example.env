# --- Pathing (The Portal) ---
CONFIG_ROOT=/var/lib/localaistack-configs
CERT_DIR=/data

# --- Service User ---
SERVICE_UID=1100
SERVICE_USER=ai-svc

# --- Domain & Security ---
BASE_DOMAIN=your.fqdn.com
CLOUDFLARE_TUNNEL_TOKEN=your_token_here
TLS_CLIENT_CERT_NAME=somefilename.pem
TLS_CLIENT_KEY_NAME=someotherfilename.pem
CLOUDFLARE_AUTH_ORIGIN_PULL_CERT_NAME=athirdfilename.pem
CLOUDFLARE_EMAIL=youremail@provider.com

# --- IDENTITY & NETWORK ---
WEBUI_SECRET=generate_a_long_random_hex
WEBUI_ADMIN_EMAIL=some-email.gmail.com
WEBUI_ADMIN_PASSWORD=supersecretpassword

# --- GOOGLE OAUTH ---
GOOGLE_ID=your_id.apps.googleusercontent.com
GOOGLE_SECRET=your_client_secret

# --- DATABASE (LangGraph) ---
POSTGRES_USER=pguser_admin
POSTGRES_PASSWORD=secure_db_password
POSTGRES_DB=langgraph_state

# --- SEARCH & TRACING (New Secrets) ---
# SearXNG needs a secret to sign its internal requests
SEARXNG_SECRET=another_random_hex
# Phoenix tracing auth (if enabled)
PHOENIX_SECRET=third_random_hex